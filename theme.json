						}
					}
				},
				"spacing": {
					"margin": "0"
				},
				"typography": {
					"fontStyle": "italic"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--md)",
					"fontWeight": "700"
				}
			}
		},
		"elements": {
